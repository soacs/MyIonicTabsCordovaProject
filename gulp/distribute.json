{
  "app": {
    "name": "MyIonicTabsCordovaProject",
    "label": "MyIonicTabsCordovaProject"
  },
  "sources": {
    "compileCommand": "grunt build:production",
    "compilePath": "src",
    "htmlVersionPath": "src/html/login.html"
  },
  "cordova": {
    "rootPath": "app"
  },
  "buildsDir": "builds/",
  "ios": {
    "bundleId": "it.lcaprini.test",
    "exportOptionsPlist": {
      "method": "enterprise",
      "teamID": "ABC123EF"
    }
  },
  "android": {
    "bundleId": "it.lcaprini.test",
    "keystore": {
      "path": "resources/android/lcaprini.keystore",
      "alias": "lcaprini-alias",
      "password": "lcaprini-password"
    }
  },
  "remote": {
    "builds": {
      "host": "lcapriniftp",
      "user": "lcaprini-user",
      "password": "lcaprini-password",
      "iosDestinationPath": "/var/www/html/test/iOS",
      "androidDestinationPath": "/var/www/html/test/Android"
    },
    "repo": {
      "host": "lcapriniftp",
      "user": "lcaprini-user",
      "password": "lcaprini-password",
      "jsonPath": "/var/www/html/test/wd",
      "iosUrlPath": "https://lcaprini.com/test/iOS",
      "androidUrlPath": "https://lcaprini.com/test/Android",
      "homepageUrl": "https://lcaprini.com/test/wd"
    },
    "sources": {
      "host": "lcapriniftp",
      "user": "lcaprini-user",
      "password": "lcaprini-passwd",
      "sourcesPath": "/var/www/html/test/sources"
    }
  },
  "email": {
    "host": "mail.gmail.com",
    "user": "lcaprini-user",
    "password": "lcaprini-password",
    "from": "luca.caprini@gmail.com",
    "to": [
      "luca.caprini@gmail.com"
    ]
  }
}
